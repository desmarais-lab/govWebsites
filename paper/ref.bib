journal = {American Political Science Review},
number = {03},
pages = {484--501},
title = {{How the Chinese Government Fabricates Social Media Posts for Strategic Distraction, Not Engaged Argument}},
url = {https://www.cambridge.org/core/product/identifier/S0003055417000144/type/journal{\_}article},
volume = {111},
